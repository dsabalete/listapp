{% extends '::base.html.twig' %}

{% block body -%}

    <div class="jumbotron">
        <h1>Nueva Tarea</h1>
    </div>

	<form action="{{ path('task_create') }}" method="post" {{ form_enctype(form) }}>
	    {{ form_errors(form) }}
		{{ form_row(form.task) }}
		<div class="hidden">{{ form_widget(form.complete) }}</div>
		{{ form_rest(form) }}

	</form>

<ul class="pager">
    <li><a href="{{ path('task') }}">Volver a la lista</a></li>
</ul>
{% endblock %}
